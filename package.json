{
  "scripts": {
    "start": "cypress open --e2e",
    "cy": "cypress run",
    "cy:alive": "cypress run --spec cypress/e2e/alive.cy.js -b chrome --reporter mochawesome",
    "cy:chrome": "cypress run -b chrome",
    "cy:firefox": "cypress run -b firefox",
    "demo": "CYPRESS_NO_COMMAND_LOG=1 cypress open --e2e --env demo=true"
  },
  "dependencies": {
    "cypress": "^12.5.1"
  },
  "devDependencies": {
    "cypress-network-idle": "^1.14.2",
    "mochawesome": "^7.1.3",
    "netlify-plugin-cypress": "^2.2.1"
  }
}
